{
    "name": "netherview",
    "version": "0.2.7",
    "description": "A character planner for Baldur's Gate 3",
    "main": "src/pages/_app.tsx",
    "scripts": {
        "test": "jest --passWithNoTests",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "format": "prettier --write .",
        "prepare": "husky install",
        "dev": "npm run version-file && next dev",
        "build": "npm run version-file && next build",
        "start": "npm run version-file && next start",
        "type-check": "tsc --noEmit --skipLibCheck --jsx react",
        "check": "npm run type-check && npm run lint-fix",
        "version-file": "ts-node scripts/create-version-file.ts",
        "preinstall": "echo \"@jorgenswiderski:registry=https://npm.pkg.github.com/\n//npm.pkg.github.com/:_authToken=${GITHUB_PACKAGES_TOKEN}\" > .npmrc",
        "link": "npm i @jorgenswiderski/tomekeeper-shared@latest && npm link @jorgenswiderski/tomekeeper-shared"
    },
    "author": "Jorgen Swiderski",
    "license": "MIT",
    "devDependencies": {
        "@babel/plugin-transform-react-jsx": "^7.22.15",
        "@babel/preset-react": "^7.22.15",
        "@babel/preset-typescript": "^7.23.2",
        "@types/crypto-js": "^4.1.3",
        "@types/d3": "^7.4.1",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.8.2",
        "@types/pako": "^2.0.2",
        "@types/react": "^18.2.25",
        "@types/react-dom": "^18.2.11",
        "@types/react-lazyload": "^3.2.2",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "@typescript-eslint/parser": "^6.7.4",
        "babel-plugin-import": "^1.13.8",
        "eslint": "^8.50.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "lint-staged": "^14.0.1",
        "mwn": "^2.0.1",
        "prettier": "^3.0.3",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@fontsource/roboto": "^5.0.8",
        "@hotjar/browser": "^1.0.9",
        "@jorgenswiderski/tomekeeper-shared": "^1.0.23",
        "@mui/icons-material": "^5.14.13",
        "@mui/material": "^5.14.12",
        "@sentry/nextjs": "^7.77.0",
        "axios": "^1.5.1",
        "axios-retry": "^3.8.1",
        "base-x": "^4.0.0",
        "crypto-js": "^4.2.0",
        "d3": "^7.8.5",
        "next": "^13.5.4",
        "pako": "^2.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-ga4": "^2.1.0",
        "react-lazyload": "github:twobin/react-lazyload",
        "react-spinners": "^0.13.8",
        "sitemap": "^7.1.1"
    }
}
